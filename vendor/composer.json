{
    "name": "laravel/tupppai",
    "description": "The Tupppai Lumen Framework.",
    "keywords": ["framework", "tupppai", "lumen", "jq"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "filp/whoops": "~1.0",
        "predis/predis": "~1.0",
        "illuminate/redis": "^5.1",
        "pda/pheanstalk": "~3.0",
        "vluzrmos/collective-html": "^1.0",
        "doctrine/dbal": "^2.5",
        "illuminate/mail": "^5.1",
	    "emojione/emojione": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "../app/"
        },
        "classmap": [
            "../database/",
            "../library/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir" : "./"
    },
    "repositories": [
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        }
    ]
}
